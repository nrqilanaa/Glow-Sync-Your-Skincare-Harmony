                android:layout_height="129dp"
                android:backgroundTint="#dda0dd "
                app:cardCornerRadius="30dp"
                app:layout_constraintBottom_toBottomOf="@+id/cardView4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.172">

                <androidx.cardview.widget.CardView
                    android:layout_width="265dp"
                    android:layout_height="97dp"
                    android:layout_marginLeft="95dp"
                    android:layout_marginTop="15dp"
                    app:cardCornerRadius="30dp">

                    <TextView
                        android:id="@+id/textView7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:text="NIACINAMIDE"
                        android:textColor="#000000"
                        android:textSize="17sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="50dp"
                        android:text="Say bye to your &quot;Bad Skin Days&quot;"
                        android:textColor="#000000"
                        android:textSize="17sp"
                        android:textStyle="italic" />

                </androidx.cardview.widget.CardView>

                <ImageView
                    android:id="@+id/imageView23"
                    android:layout_width="82dp"
                    android:layout_height="80dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="25dp"
                    app:srcCompat="@drawable/serum" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView2"
                android:layout_width="375dp"
                android:layout_height="129dp"
                android:backgroundTint="#dda0dd "
                app:cardCornerRadius="30dp"
                app:layout_constraintBottom_toBottomOf="@+id/cardView4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.393">

                <androidx.cardview.widget.CardView
                    android:layout_width="265dp"
                    android:layout_height="97dp"
                    android:layout_marginLeft="95dp"
                    android:layout_marginTop="15dp"
                    app:cardCornerRadius="30dp">

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:text="ALOE VERA"
                        android:textColor="#000000"
                        android:textSize="17sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="50dp"
                        android:text="Wave Goodbye to Your itch!"
                        android:textColor="#000000"
                        android:textSize="17sp"
                        android:textStyle="italic" />

                </androidx.cardview.widget.CardView>

                <ImageView
                    android:id="@+id/imageView37"
                    android:layout_width="74dp"
                    android:layout_height="75dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="25dp"
                    app:srcCompat="@drawable/aloe_vera" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="375dp"
                android:layout_height="129dp"
                android:backgroundTint="#dda0dd "
                app:cardCornerRadius="30dp"
                app:layout_constraintBottom_toBottomOf="@+id/cardView4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.444"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.835">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView9"
                    android:layout_width="265dp"
                    android:layout_height="97dp"
                    android:layout_marginLeft="95dp"
                    android:layout_marginTop="15dp"
                    app:cardCornerRadius="30dp">

                    <TextView
                        android:id="@+id/textView22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:text="SALICYLIC ACID"
                        android:textColor="#000000"
                        android:textSize="17sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView23"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="50dp"
                        android:text="Meet your new partner in crime"
                        android:textColor="#000000"
                        android:textSize="17sp"
                        android:textStyle="italic" />
                </androidx.cardview.widget.CardView>

                <ImageView
                    android:id="@+id/imageView25"
                    android:layout_width="85dp"
                    android:layout_height="94dp"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="20dp"
                    app:srcCompat="@drawable/acid_test" />

            </androidx.cardview.widget.CardView>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
